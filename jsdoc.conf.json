{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.(js|jsx|ts|tsx)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown", "better-docs/component", "better-docs/category", "better-docs/jsdoc"],
  "templates": {
    "search": true,
    "better-docs": {
      "name": "Asynchronous Learning Management System @ ODU-EMSE",
      "logo": "",
      "title": "ODU ALMP Docs",
      "hideGenerator": true,
      "navLinks": [
        {
          "label": "Github - UI",
          "href": "https://github.com/odu-emse/emsePortal"
        },
        {
          "label": "Github - API",
          "href": "https://github.com/odu-emse/emseAPI"
        }
      ]
    }
  },
  "opts": {
    "destination": "docs",
    "recurse": true,
    "verbose": true,
    "readme": "README.md",
    "template": "node_modules/better-docs"
  }
}