		"major": "npm run release -- --release-as major",
		"addTag": "git push --follow-tags origin dev",
		"lint": "eslint src/**/*.js src/**/*.jsx",
		"lint:fix": "eslint src/**/*.js src/**/*.jsx --fix",
		"format:watch": "npx onchange \"src/**/*.{js,jsx,ts,tsx}\" -- prettier --write --ignore-unknown {{changed}}"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
